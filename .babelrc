{
	"env": {
		"development": {
			"presets": [
				[
					"@vue/app",
					{
						"useBuiltIns": "entry"
					}
				],
				[
					"@babel/env",
					{
						"modules": false,
						"targets": {
							"browsers": "> 0.05%, not ie < 10, not op_mini all",
							"node": "current"
						}
					}
				]
			],
		},
		"test": {
			"presets": [
				"@babel/preset-env",
				{
					"targets": {
						"node": "current"
					}
				}
			],
		}
	}
}